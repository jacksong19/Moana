# .env.example
# Database
DATABASE_URL=postgresql+asyncpg://moana:moana123@localhost:5432/moana

# === Provider 选择（运行时可切换） ===
LLM_PROVIDER=gemini                  # gemini | claude | qwen
IMAGE_PROVIDER=minimax               # minimax | qwen | flux
TTS_PROVIDER=qwen                    # qwen | minimax | fish_speech
MUSIC_PROVIDER=minimax               # minimax | suno
VIDEO_PROVIDER=wanx                  # wanx | minimax

# === LLM - Google Gemini (主力) ===
GOOGLE_API_KEY=your_google_api_key
GOOGLE_MODEL=gemini-3-pro-preview

# === LLM - Anthropic Claude (备选) ===
ANTHROPIC_API_KEY=your_anthropic_api_key
ANTHROPIC_MODEL=claude-sonnet-4-20250514

# === 阿里云 DashScope (Qwen 系列 + 万相) ===
DASHSCOPE_API_KEY=your_dashscope_api_key
DASHSCOPE_API_BASE=https://dashscope.aliyuncs.com/api/v1
# Qwen LLM (备选)
QWEN_LLM_MODEL=qwen3-max-preview
# Qwen 图像生成 (备选)
QWEN_IMAGE_MODEL=qwen-image-plus
# Qwen TTS (主力)
QWEN_TTS_MODEL=qwen3-tts-flash-realtime
# 阿里万相视频 (主力)
WANX_VIDEO_MODEL=wan2.5-i2v-preview
WANX_VIDEO_SIZE=832*480
WANX_VIDEO_DURATION=5

# === MiniMax ===
MINIMAX_API_KEY=your_minimax_api_key
MINIMAX_API_BASE=https://api.minimaxi.com
# MiniMax 音乐 (主力)
MINIMAX_MUSIC_MODEL=music-2.0
# MiniMax 图像 (主力)
MINIMAX_IMAGE_MODEL=image-01
# MiniMax TTS (备选)
MINIMAX_TTS_MODEL=speech-2.6-turbo
# MiniMax 视频 (备选)
MINIMAX_VIDEO_MODEL=Hailuo-2.3-Fast

# === Flux 图像生成 (备选) ===
FLUX_API_KEY=your_flux_api_key
FLUX_API_BASE=https://api.bfl.ml

# === Midjourney (预留) ===
MIDJOURNEY_API_KEY=your_midjourney_api_key
MIDJOURNEY_API_BASE=https://api.midjourney.com

# === TTS - Fish Speech (备选) ===
FISH_SPEECH_API_KEY=your_fish_speech_api_key
FISH_SPEECH_API_BASE=https://api.fish.audio

# === Suno 音乐 (预留) ===
SUNO_API_KEY=
SUNO_API_BASE=

# === Storage - OSS/S3 ===
OSS_ACCESS_KEY=your_oss_access_key
OSS_SECRET_KEY=your_oss_secret_key
OSS_BUCKET=moana-content
OSS_ENDPOINT=https://oss-cn-hangzhou.aliyuncs.com
